<%= bootstrap_form_for("export", :url => '/views/reports/export.html.erb', :html => { :class => "form-horizontal"}) do |f| %>
    <div class="well">         <!--Temporary fix- Removed class col-md-6 col-md-offset-3-->
      <fieldset>
        <div class="col-md-8 col-md-offset-2">
          <legend>Export your Diary Entries</legend>

          <div class="form-group">
            <%= f.text_field :email, label: "Email:", placeholder: "john.doe@gmail.com" %>

          </div>

          <div class="form-actions">
            <%= f.submit "Send"%>
          </div>

        </div>
      </fieldset>
<% end %>
</div>